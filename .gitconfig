[core]
	excludesfile = ~/.gitignore
[user]
	email = caleb@alerque.com
	name = Caleb Maclennan
[github]
	user = alerque
[pld]
	user = caleb@pld-linux.org
[push]
	default = matching
[color]
	ui = true
[merge]
	ff = false
[branch]
	autosetuprebase = always
	rebase = preserve
[pull]
	rebase = true
[rebase]
	autosquash = true
[alias]
	graph = log --all --graph --decorate --oneline
	ds = diff --staged
	co = checkout
[log]
	abbrevCommit = true
