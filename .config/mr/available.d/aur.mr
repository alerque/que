#!/usr/bin/env zsh

yaourt -Ss --maintainer caleb |
	sed -n -e '/^aur/{s#.*/##;s# .*##;p}' |
	cat <<< "
		mattermost
		otf-libertinus
		otf-libertinus-git
		otf-montserrat
		sile
	" |
	sed -e '/^[[:space:]]*$/d' |
	while read pkg; do
		cat <<- EOF
			[/home/caleb/projects/aur/${pkg}]
			checkout = git clone ssh://aur@aur.archlinux.org/${pkg}.git
		EOF
	done

